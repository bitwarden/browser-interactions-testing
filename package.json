{
  "name": "@bitwarden/autofill-playwright",
  "version": "0.0.0",
  "description": "Bitwarden Autofill Playwright Tests",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitwarden/autofill-playwright.git"
  },
  "author": "Bitwarden Inc. <hello@bitwarden.com> (https://bitwarden.com)",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/bitwarden/autofill-playwright/issues"
  },
  "homepage": "https://bitwarden.com",
  "devDependencies": {
    "@playwright/test": "1.35.1",
    "@types/chrome": "0.0.237",
    "@types/firefox-webext-browser": "111.0.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "tldjs": "2.3.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "dotenv": "16.3.1",
    "type-fest": "2.19.0"
  },
  "lint-staged": {
    "*": "prettier --cache --write --ignore-unknown"
  },
  "scripts": {
    "prepare": "husky install",
    "pretest": "rimraf tests-out && tsc --incremental -p tests/tsconfig.json",
    "test:autofill": "npm run pretest && playwright test --reporter=line",
    "test:autofill:debug": "npm run pretest && playwright test --debug"
  }
}
